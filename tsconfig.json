{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"strict": true,
		"alwaysStrict": true,
		"moduleDetection": "force",
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"skipLibCheck": true,
		"inlineSourceMap": true,
		"noImplicitAny": true
	},
	"include": [
		"**/*.ts"
	]
}